#include <stdio.h>

int main(void) {
    int A, B, C;
    scanf("%d %d", &A, &B);
    scanf("%d", &C);
    if ((B + C) < 60)
        printf("%d %d", A, B + C);
    else {
        A += ((B + C) / 60);
        if (A >= 24)
            A %= 24;
        printf("%d %d", A, (B + C) % 60);
    }
    return 0;
}
# 푸는 데 1년도 넘게 걸린 문제.. 간단한 건데 약간 복잡해서 그런가
